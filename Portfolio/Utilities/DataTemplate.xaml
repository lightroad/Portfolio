<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:Portfolio.ViewModels"
    xmlns:view="clr-namespace:Portfolio.Views"
    >

    <DataTemplate DataType="{x:Type vm:PortfolioVM}" >
        <view:Portfolios />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginVM}" >
        <view:Login />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CustomerVM}" >
        <view:Customers />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}" >
        <view:Settings />
    </DataTemplate>

</ResourceDictionary>
